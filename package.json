{
  "name": "vscode-jq-playground",
  "displayName": "Visual Code jq playground",
  "description": "Visual Code integration with jq",
  "version": "2.1.1",
  "publisher": "davidnussio",
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "David Nussio",
    "email": "david.nussio@gmail.com"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "bugs": {
    "url": "https://github.com/davidnussio/vscode-jq-playground/issues",
    "email": "david.nussio@gmail.com"
  },
  "homepage": "https://github.com/davidnussio/vscode-jq-playground/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidnussio/vscode-jq-playground.git"
  },
  "engines": {
    "vscode": "^1.31.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "JSON",
    "query"
  ],
  "activationEvents": [
    "onLanguage:jq",
    "onCommand:extension.openExamples",
    "onCommand:extension.openManual",
    "onCommand:extension.openTutorial",
    "onCommand:extension.openPlay",
    "onCommand:extension.runQueryOutput",
    "onCommand:extension.runQueryEditor"
  ],
  "main": "./out/extension",
  "contributes": {
    "languages": [
      {
        "id": "jq",
        "extensions": [
          ".jq"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "jq",
        "scopeName": "source.jq",
        "path": "./syntaxes/jq.tmLanguage.json",
        "embeddedLanguages": {
          "source.json": "json"
        }
      }
    ],
    "commands": [
      {
        "command": "extension.openManual",
        "title": "Manual",
        "category": "jq playground"
      },
      {
        "command": "extension.openTutorial",
        "title": "Tutorial",
        "category": "jq playground"
      },
      {
        "command": "extension.openExamples",
        "title": "Examples",
        "category": "jq playground"
      },
      {
        "command": "extension.openPlay",
        "title": "Open online jqplay",
        "category": "jq playground"
      },
      {
        "command": "extension.runQueryOutput",
        "title": "Run query in output",
        "category": "jq playground"
      },
      {
        "command": "extension.runQueryEditor",
        "title": "Run query in editor",
        "category": "jq playground"
      }
    ],
    "keybindings": [
      {
        "command": "extension.runQueryOutput",
        "key": "ctrl+enter",
        "mac": "cmd+enter",
        "when": "editorTextFocus && editorLangId == 'jq'"
      },
      {
        "command": "extension.runQueryEditor",
        "key": "shift+enter",
        "mac": "shift+enter",
        "when": "editorTextFocus && editorLangId == 'jq'"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/glob": "^7.1.1",
    "@types/node": "^13.13.1",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "babel-jest": "^25.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@types/checksum": "^0.1.30",
    "@types/download": "^6.2.4",
    "@types/jest": "^25.2.1",
    "@types/ramda": "^0.27.3",
    "checksum": "^0.1.1",
    "crocks": "^0.12.4",
    "download": "^7.1.0",
    "eyes": "^0.1.8",
    "ramda": "^0.27.0",
    "ts-jest": "^25.4.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/src/test/"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "printWidth": 80,
    "trailingComma": "all"
  }
}
